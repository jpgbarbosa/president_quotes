<p id="notice"><%= notice %></p>

<h1>Presidents</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @presidents.each do |president| %>
      <tr>
        <td><%= president.name %></td>
        <td><%= president.country %></td>
        <td><%= link_to 'Show', president %></td>
        <td><%= link_to 'Edit', edit_president_path(president) %></td>
        <td><%= link_to 'Destroy', president, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New President', new_president_path %>
